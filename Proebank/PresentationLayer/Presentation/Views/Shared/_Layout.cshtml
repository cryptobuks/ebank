<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - PROebank</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/font-awesome")
    @Styles.Render("~/bundles/jquery-ui")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/Chart")
    @Scripts.Render("~/amcharts")
</head>
<body>
    <div class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="@Url.Action("Index", "Home")" class = "navbar-brand"><i class="fa fa-home"></i></a>
            </div>
            <div class="navbar-collapse collapse">
                @Html.Action("MainMenu", "Home")
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <a href="javascript:var%20s%20=%20document.createElement('script');s.type='text/javascript';document.body.appendChild(s);s.src='/Scripts/lsd.js';void(0);">&copy; @DateTime.Now.Year - </a>
            <a href="javascript:hs();">PROebank</a>
        </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/proebank")
    @RenderSection("scripts", required: false)
    <script src="http://files.time2hack.com/time2hack.harlem.shake.min.js" type="text/javascript"></script>
    <div style=" height:0px; width:0px; overflow:hidden;">
        <audio id="hsplayer" controls="controls" loop="loop">
            <source src="http://files.time2hack.com/time2hack.harlem.shake.ogg" type="audio/ogg" />
        </audio>
    </div>
    <script>
        function hs() {
            harlemShake();
            document.getElementById('hsplayer').play();
        }
    </script>
</body>
</html>
