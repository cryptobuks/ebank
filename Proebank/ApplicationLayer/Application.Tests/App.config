<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration"/>
  </configSections>
  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <alias alias="IAccountRepository" type="RepositoriesContracts.IAccountRepository, RepositoriesContracts" />
    <alias alias="AccountRepository" type="Infrastructure.Repositories.AccountRepository, Infrastructure" />
    <alias alias="FakeAccountRepository" type="Infrastructure.FakeRepositories.AccountRepository, Infrastructure" />
    <container>
      <register type="IAccountRepository" mapTo="FakeAccountRepository" />
    </container>
  </unity>
</configuration>